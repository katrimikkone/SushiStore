package backend.sushistore.domain;

import java.sql.Blob;
import java.util.List;

import com.fasterxml.jackson.annotation.JsonIgnore;

import jakarta.persistence.CascadeType;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.OneToMany;

@Entity
public class Symbol {
	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private Long symbolid;
	private Blob address;
	
	@OneToMany(cascade = CascadeType.ALL, mappedBy = "symbol")
	@JsonIgnore
	private List<Sushi> sushis;

	public Symbol() {
		
	}

	public Symbol(Blob address) {
		this.address = address;
	}

	public Long getId() {
		return symbolid;
	}

	public void setId(Long symbolid) {
		this.symbolid = symbolid;
	}

	public Blob getAddress() {
		return address;
	}

	public void setAddress(Blob address) {
		this.address = address;
	}

	public Symbol(Long symbolid, Blob address, List<Sushi> sushis) {
		super();
		this.symbolid = symbolid;
		this.address = address;
		this.sushis = sushis;
	}

	
	
	
}
